<RuleSet
	xmlns="https://msobkow.github.io/msscf/2.0.13/xsd/cfgenkb-2.13-ruleset.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xsi:schemaLocation="https://msobkow.github.io/msscf/2.0.13/xsd/cfgenkb-2.13-ruleset.xsd https://msobkow.github.io/msscf/2.0.13/xsd/cfgenkb-2.13-ruleset.xsd"
	ToolSet="java+saxloader"
	Revision="3.0"
	Name="saxramldr .gitignore"
	Descr="Java 25 SAX RAM Loader .gitignore">

	<GenFile GenDef="SchemaDef" Name="fileSchemaSaxRamLdrGitIgnore"
			GenerateOnce="false"
		ModuleName="$reference ManufacturingSchema lower JavaPackage$.$reference ManufacturingSchema lower SchemaName$saxramldr"
		SourceBundle="java"
		BasePackageName="$reference ManufacturingSchema lower JavaPackage$.$reference ManufacturingSchema lower SchemaName$saxramldr"
		SubPackageName=""
		ExpansionClassName="SaxRamLdrDotGitIgnore"
		ExpansionKeyName="$SchemaName$SaxRamLdrDotGitIgnore"
		ExpansionFileName=".gitignore"
>marktemp
# Temp files
installer
*.swp
*~
.metadata
RemoteSystemsTempFiles
# Netbeans configuration
Servers

# Compiled class file
*.class

# Build directory
build
/target/

# Log file
*.log

# BlueJ files
*.ctxt

# Mobile Tools for Java (J2ME)
.mtj.tmp/

# Package Files #
*.jar
*.war
*.nar
*.ear
*.zip
*.tar.gz
*.rar

# virtual machine crash logs, see http://www.java.com/en/download/help/error_hotspot.xml
hs_err_pid*
replay_pid*
</GenFile>

</RuleSet>
