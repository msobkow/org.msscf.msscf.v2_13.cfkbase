 <RuleSet
	xmlns="http://msscf.org:8088/msscf/2.0.13/xsd/cfgenkb-2.13-ruleset.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xsi:schemaLocation="http://msscf.org:8088/msscf/2.0.13/xsd/cfgenkb-2.13-ruleset.xsd http://msscf.org:8088/msscf/2.0.13/xsd/cfgenkb-2.13-ruleset.xsd"
	ToolSet="any"
	Name="FileHeaders"
	Revision="3.0"
	Descr="File header fragments">

	<GenRule GenDef="Object" Name="FileStartHeader">/* *********************************************************************
</GenRule>

	<GenRule GenDef="Object" Name="ScriptStartHeader"># **********************************************************************
</GenRule>

	<GenRule GenDef="Object" Name="BatScriptStartHeader">REM *********************************************************************
</GenRule>

	<GenRule GenDef="Object" Name="DoubleDashScriptStartHeader">-- **********************************************************************
</GenRule>

	<GenRule GenDef="Object" Name="MssSourceLicense"
>/*
 *	$poptop SchemaDef ProjectDescription$
 *
 *	Copyright (c) $EffectiveCopyrightPeriod$ $EffectiveCopyrightHolder$
$prefixline SpcAstTab poptop SchemaDef CommonLicenseText$
 *
 *	Manufactured by MSS Code Factory
 */</GenRule>

	<GenRule GenDef="Object" Name="MssBashScriptLicense"
>#
#	$poptop SchemaDef ProjectDescription$
#
#	Copyright (c) $EffectiveCopyrightPeriod$ $EffectiveCopyrightHolder$
$prefixline PoundTab poptop SchemaDef CommonLicenseText$
#
#	Manufactured by MSS Code Factory
#</GenRule>

	<GenRule GenDef="Object" Name="PoundTab">#	</GenRule>

	<GenRule GenDef="Object" Name="MssBatScriptLicense"
>REM
REM	$poptop SchemaDef ProjectDescription$
REM
REM	Copyright (c) $EffectiveCopyrightPeriod$ $EffectiveCopyrightHolder$
$prefixline RemTab poptop SchemaDef CommonLicenseText$
REM
REM Manufactured by MSS Code Factory
REM</GenRule>

	<GenRule GenDef="Object" Name="RemTab">REM	</GenRule>

	<GenRule GenDef="Object" Name="MssDoubleDashScriptLicense"
>--
--	$poptop SchemaDef ProjectDescription$
--
--	Copyright (c) $EffectiveCopyrightPeriod$ $EffectiveCopyrightHolder$
$prefixline DoubleDashTab poptop SchemaDef CommonLicenseText$
--
--	Manufactured by MSS Code Factory
--</GenRule>

	<GenRule GenDef="Object" Name="DoubleDashTab">--	</GenRule>

</RuleSet>
