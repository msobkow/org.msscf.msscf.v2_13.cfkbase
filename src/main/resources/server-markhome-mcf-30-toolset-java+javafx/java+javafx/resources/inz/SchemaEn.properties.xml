<RuleSet
		xmlns="http://msscf.org:8088/msscf/2.0.13/xsd/cfgenkb-2.13-ruleset.xsd"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:xs="http://www.w3.org/2001/XMLSchema"
		xsi:schemaLocation="http://msscf.org:8088/msscf/2.0.13/xsd/cfgenkb-2.13-ruleset.xsd http://msscf.org:8088/msscf/2.0.13/xsd/cfgenkb-2.13-ruleset.xsd"
	ToolSet="java+javafx"
	Name="resources/inz/SchemaEn.properties"
	Revision="3.0"
	Descr="English language properties for the JavaFX field and attribute names">

	<GenFile GenDef="SchemaDef" Name="fileSchemaResourcesInzEnProperties"
		GenerateOnce="false"
		ModuleName="$reference ManufacturingSchema lower JavaPackage$.$reference ManufacturingSchema lower SchemaName$javafx"
		SourceBundle="java"
		BasePackageName="$reference ManufacturingSchema lower JavaPackage$.$reference ManufacturingSchema lower SchemaName$javafx/src/main/resources"
		SubPackageName="inz"
		ExpansionClassName="$SchemaName$ResourcesInzEnProperties"
		ExpansionKeyName="$SchemaName$ResourcesInzEnProperties"
		ExpansionFileName="en.properties">$iterate Tables(each enProperties )$
</GenFile>

	<GenRule GenDef="Table" Name="enProperties">$declAttrPaneEmitTags$
</GenRule>

	<GenRule GenDef="Table" Name="declAttrPaneEmitTags"
		>$switch HasSuperClassRelation yes declAttrPaneInhEmitTags default declAttrPaneBaseEmitTags$</GenRule>

	<GenRule GenDef="Table" Name="declAttrPaneInhEmitTags"
		>$reference SuperClassRelation reference ToTable declAttrPaneEmitTags$$iterate Columns( each declAttrPaneDataEmitTags empty empty )$</GenRule>

	<GenRule GenDef="Value" Name="declAttrPaneDataEmitTags"
		>$switch ColumnInPrimaryIndex no declAttrPaneEmitTags default empty$</GenRule>

	<GenRule GenDef="Table" Name="declAttrPaneBaseEmitTags"
		>$iterate Columns( each declAttrPaneEmitTags empty empty )$</GenRule>

	<GenRule GenDef="Value" Name="declAttrPaneEmitTags"
		>$switch ColumnInContainerOrNamedLookupRelation no declAttrPaneEmitTag default empty$</GenRule>

	<GenRule GenDef="Value" Name="declAttrPaneEmitTag">
$lower DefSchemaName$.javafx.$TableName$.AttrPane.$Name$.EffLabel=$EffLabel$</GenRule>

	<GenRule GenDef="Table" Name="declAttrPaneRelationTags"
		>$switch HasSuperClassRelation yes declAttrPaneInhRelationTags default declAttrPaneBaseRelationTags$</GenRule>

	<GenRule GenDef="Table" Name="declAttrPaneInhRelationTags"
		>$reference SuperClassRelation reference ToTable declAttrPaneRelationTags$$iterate Relation( each declAttrPaneFilterRelationTags empty empty )$</GenRule>

	<GenRule GenDef="Table" Name="declAttrPaneBaseRelationTags"
		>$iterate Relation( each declAttrPaneFilterRelationTags empty empty )$</GenRule>

	<GenRule GenDef="Relation" Name="declAttrPaneFilterRelationTags"
		>$switch RelationType
			Lookup declAttrPaneFilterChainRelationTags
			Parent declAttrPaneFilterChainRelationTags
			Container declAttrPaneFilterChainRelationTags
			Component declAttrPaneFilterChainRelationTags
			Children declAttrPaneFilterChainRelationTags
			default empty$</GenRule>
	<GenRule GenDef="Relation" Name="declAttrPaneFilterChainRelationTags">$switch IsChainRelation yes empty default reallyDeclAttrPaneEmitRelationTags$</GenRule>

	<GenRule GenDef="Relation" Name="reallyDeclAttrPaneEmitRelationTags">
$lower DefSchemaName$.javafx.$TableName$.AttrPane.$RelationType$$Suffix$.EffLabel=$EffLabel$</GenRule>

</RuleSet>
